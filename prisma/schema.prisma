generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Event {
  id             String   @id @default(uuid())
  fromDate       DateTime
  toDate         DateTime
  companyName    String
  companyAddress String?
  city           String?
  country        String?
  task           String?
  asmeId         String?
  asmeApp        String?
  aia            String?
  hotelName      String?
  airportName    String?
  completed      Boolean  @default(false)
  createdAt      DateTime @default(now())
}

